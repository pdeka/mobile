<h1>New situational_data</h1>

<% form_for(@situational_data) do |f| %>
  <%= f.error_messages %>

  <p>
    <%= f.label :location %><br />
    <%= f.text_field :location %>
  </p>
  <p>
    <%= f.label :accompanying_people %><br />
    <%= f.text_area :accompanying_people %>
  </p>
  <p>
    <%= f.label :significant_occurences %><br />
    <%= f.text_area :significant_occurences %>
  </p>
  <p>
    <%= f.submit "Create" %>
  </p>
<% end %>

<%= link_to 'Back', situational_datas_path %>

<h1>Listing situational_datas</h1>

<table>
  <tr>
    <th>Location</th>
    <th>Accompanying people</th>
    <th>Significant occurences</th>
  </tr>

<% for situational_data in @situational_datas %>
  <tr>
    <td><%=h situational_data.location %></td>
    <td><%=h situational_data.accompanying_people %></td>
    <td><%=h situational_data.significant_occurences %></td>
    <td><%= link_to 'Show', situational_data %></td>
    <td><%= link_to 'Edit', edit_situational_data_path(situational_data) %></td>
    <td><%= link_to 'Destroy', situational_data, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New situational_data', new_situational_data_path %>
